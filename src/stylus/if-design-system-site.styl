.if.color-badge
  IDS_TYPOGRAPHY_FontFamily_SansBold()
  white-space nowrap
  padding-left 1.75rem
  position relative
  display inline-block
  & .if.color-drop
    border 1px solid rgba($color-text-light-brown, 0.2)
    box-shadow inset 0 0 0 1px rgba(110,98,94, .05), 0 4px 8px rgba(110,98,94, 0.08)
    display inline-block
    height $size-16
    width $size-16
    border-radius 100%
    margin-right $size-spacing-4
    transform translateY(-50%)
    position absolute
    top 50%
    left 6px
    background-clip padding-box
    &.accentBlue
      background-color $color-accent-blue
    &.interActionDarkBlue
      background-color $color-interaction-dark-blue
      box-shadow inset 0 0 0 1px rgba(51,30,17, .05), 0 4px 8px rgba(51,30,17, 0.08)
    &.focus
      background-color complement($color-accent-blue)
    &.accentBlue05
      background-color rgba($color-accent-blue, 0.05)
    &.infographicLightGreen
      background-color $color-infographic-light-green
    &.infographicGreen
      background-color $color-infographic-green
    &.infographicLightBlue
      background-color $color-infographic-light-blue
    &.infographicLightYellow
      background-color $color-infographic-light-yellow
    &.infographicLightRed
      background-color $color-infographic-light-red
    &.backgroundDarkBrown
      background-color $color-text-brown
      box-shadow inset 0 0 0 1px rgba(51,30,17, .05), 0 4px 8px rgba(51,30,17, 0.08)
    &.backgroundDarkBrown10
      background-color rgba($color-text-brown, 0.1)
    &.backgroundDarkbrownTint50
      background-color tint($color-text-brown, 50%)
    &.backgroundDarkBeige
      background-color $color-background-dark-beige
    &.backgroundLightestBeige
      background-color $color-background-lightest-beige
    &.backgroundLighterBeige
      background-color $color-background-lighter-beige
    &.backgroundLightBeige
      background-color $color-background-light-beige
    &.backgroundLightBrown
      background-color $color-text-light-brown
      box-shadow inset 0 0 0 1px rgba(51,30,17, .05), 0 4px 8px rgba(51,30,17, 0.08)
    &.backgroundLightBrown10
      background-color rgba($color-text-light-brown, 0.1)
    &.backgroundLightBrown20
      background-color rgba($color-text-light-brown, 0.2)
    &.backgroundLightBrown30
      background-color rgba($color-text-light-brown, 0.3)


.if.axe.contrast
  IDS_TYPOGRAPHY_Text_Meta()
  display flex
  flex-direction column
  font-size 1rem
  position absolute
  left calc(100% + 6rem)
  top 50%
  transform translateY(-50%)

.if.axe.pass
  IDS_TYPOGRAPHY_FontFamily_SansBold()
  padding-left $size-spacing-24
  flex-grow 1
  flex-shrink 0
  color $color-status-green
  display block
  position relative
  &::before
    content ""
    position absolute
    left 0
    top 50%
    transform translateY(-50%)
    height $size-16
    width $size-16
    display inline-block
    background-size $size-icon-16 $size-icon-16
    background-repeat no-repeat
    background-position center center
    background-image url("data:image/svg+xml,%3Csvg xmlns=%27http://www.w3.org/2000/svg%27 viewBox=%270 0 32 32%27%3E%3Ctitle%3Echeck%3C/title%3E%3Cg class=%27nc-icon-wrapper%27 stroke-linecap=%27square%27 stroke-linejoin=%27miter%27 stroke-width=%273.5%27 fill=%27%23217331%27 stroke=%27%23217331%27%3E%3Cpolyline points=%272 16 11 25 30 6%27 fill=%27none%27 stroke=%27%23217331%27 stroke-miterlimit=%2710%27/%3E%3C/g%3E%3C/svg%3E")

.if.axe.fail
  IDS_TYPOGRAPHY_FontFamily_SansBold()
  padding-left $size-spacing-24
  flex-grow 1
  flex-shrink 0
  color $color-status-red
  display block
  position relative
  &::before
    content ""
    position absolute
    left 0
    top 50%
    transform translateY(-50%)
    height $size-16
    width $size-16
    display inline-block
    background-size $size-icon-16 $size-icon-16
    background-repeat no-repeat
    background-position center center
    background-image url("data:image/svg+xml,%3Csvg xmlns=%27http://www.w3.org/2000/svg%27 viewBox=%270 0 32 32%27%3E%3Ctitle%3Ee remove%3C/title%3E%3Cg class=%27nc-icon-wrapper%27 stroke-linecap=%27square%27 stroke-linejoin=%27miter%27 stroke-width=%273.5%27 fill=%27%23bb320c%27 stroke=%27%23bb320c%27%3E%3Cline fill=%27none%27 stroke=%27%23bb320c%27 stroke-miterlimit=%2710%27 x1=%2727%27 y1=%275%27 x2=%275%27 y2=%2727%27/%3E%3Cline fill=%27none%27 stroke=%27%23bb320c%27 stroke-miterlimit=%2710%27 x1=%2727%27 y1=%2727%27 x2=%275%27 y2=%275%27/%3E%3C/g%3E%3C/svg%3E")

small.if
  IDS_TYPOGRAPHY_Text_Meta()

.if.relative
  position relative


.if.axe-helper-dash::before
  content ""
  height 1pt
  width 2rem
  display block
  border-top 1pt dashed $color-text-brown
  position absolute
  top 50%
  transform translateY(-50%)
  right calc(0% - 2rem)

.if.axe-helper-dash::after
  content ""
  height 1pt
  width 3rem
  display block
  border-top 1pt dashed $color-text-brown
  position absolute
  top 50%
  transform translateY(-50%)
  left calc(100% + 2rem)

.if.axe-helper-dash.light::before
  border-top 1pt dashed $color-text-lightest-beige







