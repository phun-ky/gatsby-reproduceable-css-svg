html.sg
  min-height 100%
  /* Prevent adjustments of font size after orientation changes in iOS. */
  -webkit-text-size-adjust 100%

body.sg
  box-sizing border-box
  min-height 100vh
  line-height 28px
  background-color $color-background-light-beige


body.sg::-webkit-scrollbar
  background-color transparent
  width $size-4
  height $size-4



body.sg::-webkit-scrollbar-thumb
  background-color $color-text-light-brown
  width $size-4

*.sg,
*.sg::before,
*.sg::after
  line-height 28px
  box-sizing border-box
  margin 0
  padding 0


ol.if,
ul.if
